<% content_for :stylesheets do %>
  <%= stylesheet_link_tag "galleries" %>
<% end %>

<% content_for :after_jquery do %>
  <%= javascript_include_tag "http://gsgd.co.uk/sandbox/jquery/easing/jquery.easing.1.3.js" %>
  <%= javascript_include_tag 'jquery.cycle.all' %>
  <%= javascript_include_tag 'galleries' %>
<% end %>

<p>
  <%= link_to "&laquo; Back to portfolio".html_safe, galleries_path %> 
</p>
<h1 class="border-bot"><%= @gallery.title %></h1>
<%= render :partial => "admin_links" %>
<div class="indent-bot">
  <div id="gallery-pics">
    <%= render :partial => "pictures" %>
  </div>
</div>
